package com.Project_Z;

public class MathMain {

	public static void main(String[] args) {
		
		//Parent Child RelationShip
		Shape sp = new Circle();
		
		//Child Object Creation
		Circle cc = new Circle();
		
		//Parent Object Creation
		Square sq = new Square();
		
		//Parent Child RelationShip
		Rectangle rc = new Square();
		
		System.out.println("Area of a Square: " +sq.Area(10));
		
		System.out.println("Perimeter of a Square: " +sq.Perimeter(6));
		
		System.out.println("Area of a Rectangle: " +rc.Area(5, 8));
	
		System.out.println("Perimeter of a Rectangle: " +rc.Perimeter(5, 8));
		
		System.out.println("Area of a Circle: " +cc.Area(50));
	
		System.out.println("Perimeter of a Circle: " +cc.Perimeter(25));
		
	}

}
