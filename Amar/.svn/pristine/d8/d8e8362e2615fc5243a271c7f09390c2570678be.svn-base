package com.Project_Z;

public class Shape {

	private String color;

	public Shape() {
	}	
	public Shape(String color) {
		super();
		this.color = color;
	}

	public String getColor() {
		return color;
	}

	public void setColor(String color) {
		this.color = color;
	}

	@Override
	public String toString() {
		return "Shape [color=" + color + "]";
	}
	
	
	
	
	
	
}
