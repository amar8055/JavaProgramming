package com.jnit.oca2017;

import java.time.LocalDate;
import java.util.Date;






public class PackageExample {

	public static void main(String[] args) {
		
		String value1,value2;// Can declare two or more variables in a single line
		
		String e1="Hello", e2 ="World";// can initialize 2 or more variables in the same line of the same data type
		
		//String v1,String v2; // the data type declaration should only be once
		
		int x1 = 54, x2,x3 ;// can initialize only one variable and just declare other variables
		
		int x = 100_00;
		
		//int y = _2456;
		
		
		float f = 123.67_89f;
		
		//double d = 347_.589;
		
		//System.out.println(d);
		
		System.out.println(f);
		
		LocalDate localDate = LocalDate.now();// Package should be imported
		
		Date utilDate = new Date();
		
		java.sql.Date sqlDate = new java.sql.Date(x); 
		
		double d = Math.PI;// Java.lang is automatically imported 
			
		System.out.println(d);
		
		System.out.println(localDate);
		
		System.out.println(utilDate);
		

	}
}
