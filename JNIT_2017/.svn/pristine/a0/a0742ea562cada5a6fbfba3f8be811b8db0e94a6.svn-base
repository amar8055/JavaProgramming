package com.jnit.oca2017;

public class Operators {

	public static void main(String[] args) {

		int x = 2 * 10 + 2 * 4 / 2 - 2;// if the operators are of the same
										// level, expression evaluation starts
										// from left to rigt
		System.out.println(x);
		
		int y = 2 * (10 + 2 * 4 / 2) - 2;
		System.out.println(y);// the order of evaluation changes when provided with parentheses
		
		
		System.out.println(16/4);
		System.out.println(19/4);
		System.out.println(19%4);

	}

}
