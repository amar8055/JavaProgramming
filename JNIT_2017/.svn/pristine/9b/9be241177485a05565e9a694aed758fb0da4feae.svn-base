package com.jnit.oca2017.constructors;

public class LapDog extends Dog {

	boolean isLapDog;

	public LapDog(boolean isLapDog) {

		this.isLapDog = isLapDog;
		
		System.out.println(this.name);
		System.out.println(this.age);
		System.out.println(super.breed);
	}

	
	public static void main(String[] args) {
		
		LapDog lp = new LapDog(true);
		
	}
}
